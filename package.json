{
  "name": "gatsby-prismic-starter-prist",
  "private": true,
  "description": "A starter powered by Gatsby and Prismic to showcase portfolios and blogs.",
  "version": "0.1.0",
  "author": "Marguerite Roth | marguerite.io | marguerite.roth@gmail.com",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:prettier",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:css": "stylelint 'src/**/*.js'",
    "lint:prettier": "prettier -l {,src/**/}*.{js,jsx,json,css,scss}",
    "lint:prettier:format": "prettier --write \"src/**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.12",
    "gatsby": "^2.12.0",
    "gatsby-image": "^2.1.2",
    "gatsby-plugin-catch-links": "^2.0.12",
    "gatsby-plugin-emotion": "^4.1.0",
    "gatsby-plugin-google-analytics": "^2.1.6",
    "gatsby-plugin-manifest": "^2.2.4",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.1.3",
    "gatsby-source-filesystem": "^2.0.38",
    "gatsby-source-prismic-graphql": "3.4.0-beta.1",
    "gatsby-transformer-sharp": "^2.1.21",
    "node-sass": "^4.12.0",
    "prismic-reactjs": "^0.3.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.8.6",
    "react-emoji": "^0.5.0",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.0.6",
    "react-modal": "^3.8.1",
    "react-moment": "^0.9.2",
    "react-router-dom": "^5.0.0",
    "react-router-hash-link": "^1.2.1",
    "smooth-scroll": "^16.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-eslint": "^10.0.3",
    "dotenv": "^8.1.0",
    "eslint": "^6.4.0",
    "eslint-config-hyam": "https://github.com/hyamstudios/eslint-config-hyam.git",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react-hooks": "^2.0.1",
    "gatsby-plugin-remove-trailing-slashes": "^2.0.11",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.17.1"
  },
  "keywords": [
    "gatsby",
    "prismic",
    "blog",
    "portfolio"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/margueriteroth/gatsby-prismic-starter-prist"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
